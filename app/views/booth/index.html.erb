<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<h1>Voting Booth</h1>

<% @hacks.each do |hack| %>
    <div class = "entry">
        <h3><%= hack.title %></h3>
        <%= sanitize(hack.hackers) %>
        <%= button_to 'Vote Up!', votes_path(hack_id: hack) %>
        <%= button_to 'Vote Down!', votes_path(hack_id: hack) %>
    </div>
<% end %>
    
    